# Navigation Aliases
alias back='cd -'

# Git Aliases #
alias gutcheck="git status"
alias boom="git reset head --hard"
alias pretty="!. ~/.githelpers && pretty_git_log"

# File Aliases
alias zshconfig="open ~/.zshrc"
alias ohmyzsh="open ~/.oh-my-zsh"
alias aliases="open ~/.aliases"
alias config="cd ~/;open .zshrc"

# Folder Aliases
alias dot="cd ~/Development/dotfiles"
alias usr="cd ~"
alias usrlocal="cd /usr/local/"
alias dev="cd ~/Development/"
alias web="cd ~/Development/Web"
alias ios="cd ~/Development/iOS/"
alias multi="cd ~/Development/MultiPlatform/"
alias downloads="cd ~/Downloads/"
alias desktop="cd ~/Desktop/"
alias docs="cd ~/Documents/"
alias dropbox="cd ~/Dropbox/"
alias trash="cd ~/.Trash"

# Program Aliases
alias chrome="open /Applications/Google\ Chrome.app"
alias xcode="open /Applications/xcode.app"
alias studio="open /Applications/Android\ Studio.app"
alias adium="open /Applications/Adium.app"
alias slack="open /Applications/Slack.app"
alias cord="open /Applications/Cord.app"
alias vm="open /Applications/VMWare\ Fusion.app"

# Android Aliases
alias initconnect="adb tcpip 5555"
alias connect="adb connect 192.168.1.105:5555"	